{
  "name": "@aavegotchi/aavegotchi-cli",
  "version": "0.1.0",
  "description": "Agent-first CLI for automating Aavegotchi app and onchain workflows",
  "license": "MIT",
  "repository": "git@github.com:aavegotchi/aavegotchi-cli.git",
  "type": "commonjs",
  "main": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "bin": {
    "ag": "dist/index.js",
    "aavegotchi-cli": "dist/index.js"
  },
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "parity:check": "node scripts/check-parity.mjs",
    "ag": "tsx src/index.ts",
    "prepack": "npm run build",
    "bootstrap:smoke": "AGCLI_HOME=/tmp/agcli-smoke tsx src/index.ts bootstrap --mode agent --profile smoke --chain base --signer readonly --json"
  },
  "dependencies": {
    "better-sqlite3": "^11.8.1",
    "pino": "^9.11.0",
    "viem": "^2.39.3",
    "yaml": "^2.8.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^22.13.10",
    "tsx": "^4.20.5",
    "typescript": "^5.8.2",
    "vitest": "^4.0.18"
  }
}
